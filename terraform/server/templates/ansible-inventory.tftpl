[all:vars]
domain = ${ network.domain }

[proxmox]
${ proxmox.node } ansible_host=${ proxmox.ip }

[dns]
${ dns.container.hostname } ansible_host=${ dns.container.ip } vmid=${ dns.container.vmid } proxmox_node=${ dns.container.node }

[services]
