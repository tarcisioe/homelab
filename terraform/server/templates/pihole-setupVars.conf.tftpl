PIHOLE_INTERFACE=eth0
QUERY_LOGGING=true
INSTALL_WEB_INTERFACE=true
CACHE_SIZE=10000
DNS_FQDN_REQUIRED=true
DNS_BOGUS_PRIV=true
DNSMASQ_LISTENING=all
BLOCKING_ENABLED=true
REV_SERVER=false
DNSSEC=false

PIHOLE_DOMAIN=${ network.domain }

DHCP_ACTIVE=true
DHCP_START=${ dhcp.start }
DHCP_END=${ dhcp.end }
DHCP_ROUTER=${ dhcp.router }
DHCP_LEASETIME=24
DHCP_IPv6=false
DHCP_rapid_commit=false

TEMPERATUREUNIT=C

PIHOLE_DNS_1=1.1.1.1
PIHOLE_DNS_2=1.0.0.1
PIHOLE_DNS_3=8.8.8.8
PIHOLE_DNS_4=8.8.4.4
